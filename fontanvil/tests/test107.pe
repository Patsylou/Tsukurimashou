#!/usr/local/bin/fontanvil
#Needs: fonts/Ambrosia*.sfd
#Make sure we can generate a font family

srcdir=GetEnv("srcdir")
if (srcdir=="")
  srcdir=""
else
  srcdir=srcdir+"/"
endif

helper107.pe(srcdir+"tests/fonts/Ambrosia.sfd", \
  srcdir+"tests/fonts/AmbrosiaBold.sfd", \
  srcdir+"tests/fonts/AmbrosiaItalic.sfd");
GetCoverageCounts()
