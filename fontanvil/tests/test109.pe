#!/usr/local/bin/fontanvil
#Needs: fonts/OverlapBugs.sfd
#Needs: fonts/feta20.pfb
#Make sure remove overlap sort of works...

srcdir=GetEnv("srcdir")
if (srcdir=="")
  srcdir=""
else
  srcdir=srcdir+"/"
endif

Open(srcdir+"tests/fonts/OverlapBugs.sfd")
SelectAll();
RemoveOverlap();

Open(srcdir+"tests/fonts/feta20.pfb");
SelectAll();
RemoveOverlap();
GetCoverageCounts()
